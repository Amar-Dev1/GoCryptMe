{
  "name": "gocryptme",
  "version": "1.0.0",
  "description": "Offline password encryption app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "npx electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Amar-Dev1/GoCryptMe.git"
  },
  "keywords": [],
  "author": {
    "name": "Amar Official",
    "email": "amarofficial249@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Amar-Dev1/GoCryptMe/issues"
  },
  "homepage": "https://github.com/Amar-Dev1/GoCryptMe#readme",
  "dependencies": {
    "electron-store": "^10.1.0"
  },
  "devDependencies": {
    "electron": "^37.2.1",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.gocryptme.app",
    "productName": "GoCryptMe",
    "publish": {
    "provider": "github",
    "overwrite": true
  },
    "directories": {
      "buildResources": "build"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.png",
      "executableName": "GoCryptMe"
    },
    "linux": {
      "icon": "build/icon.png",
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility"
    },
    "mac": {
      "target": "dmg"
    }
  }
}
